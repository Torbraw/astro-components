---
import { twMerge } from 'tailwind-merge';
type Props = {
  class?: string;
  'class:list'?: string;
};
const { class: className = '', 'class:list': classList = '' } = Astro.props;
---

<tr class:list={twMerge(['border-b transition-colors hover:bg-muted/50 data-[state=selected]:bg-muted', className, classList])}>
  <slot />
</tr>
