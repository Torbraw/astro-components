---
import { twMerge } from 'tailwind-merge';
export type Props = {
  class?: string;
  'class:list'?: string;
};
const { class: className = '', 'class:list': classList = '' } = Astro.props;
---

<div class="w-full overflow-auto">
  <table class:list={twMerge(['w-full text-sm', className, classList])}>
    <slot />
  </table>
</div>
