---
import { twMerge } from 'tailwind-merge';
export type Props = {
  class?: string;
  'class:list'?: string;
};
const { class: className = '', 'class:list': classList = '' } = Astro.props;
---

<h3 class:list={twMerge(['font-semibold leading-none tracking-tight', className, classList])}>
  <slot />
</h3>
